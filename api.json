---
layout: null
---
[
  {% for api in site.api %}
    {
      "title" : "{{ api.name }}",
      "url": "{{ api.url | prepend: site.baseurl }}",
      "description" : "{{ api.tagline }}",
      "image" : "{{ api.logo | prepend: site.baseurl }}",
      "domain" : "{{ api.domain }}",
      "owner" : "{{ api.owner }}",
      "contract" : "{{ api.contract }}",
      "restriction" : "{{ api.restriction }}",
      "type" : "{{ api.type }}",
      "keywords": [
        {% for keyword in api.keywords %}
        "{{ keyword }}"
        {% if forloop.last == false %}
        ,
        {% endif %}
      {% endfor %}
      ],
      "clients": [
        {% for client in api.clients %}
        "{{ client }}"
        {% if forloop.last == false %}
        ,
        {% endif %}
      {% endfor %}
      ],
      "partners": [
        {% for partner in api.partners %}
        "{{ partner }}"
        {% if forloop.last == false %}
        ,
        {% endif %}
      {% endfor %}
      ]
    }
    {% if forloop.last == false %}
    ,
    {% endif %}
  {% endfor %}
]
