---
layout: null
---
[
  {% for api in site.api %}
    {
      "title" : "{{ api.name }}",
      "url": "{{ api.url | prepend: site.baseurl }}",
      "description" : "{{ api.tagline }}",
      "image" : "{{ api.logo | prepend: site.baseurl }}",
      "domain" : "{{ api.domain }}",
      "owner" : "{{ api.owner }}",
      "contract" : "{{ api.contract }}",
      "restriction" : "{{ api.restriction }}",
      "type" : "{{ api.type }}",
      "keywords": [
        "{{ api.keyword | join: '","' }}"
      ],
      "clients": [
        "{{ api.clients | join: '","' }}"
      ],
      "partners": [
        "{{ api.partners | join: '","' }}"
      ]
    }
    {% if forloop.last == false %}
    ,
    {% endif %}
  {% endfor %}
]
